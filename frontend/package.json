{
  "name": "develop",
  "version": "0.0.0",
  "scripts": {
    "all": "npm run all-without-test && npm run test",
    "all-without-test": "npm run lint && npm run build-check",
    "test": "ng test --no-watch --no-progress --browsers ChromeHeadlessCI",
    "test-browser": "ng test --watch=true --poll 2000 --code-coverage",
    "lint": "",
    "lint-skipped": "eslint --fix \"src/**/*.ts\" \"test/**/*.ts\" && eslint \"src/**/*.ts\" \"test/**/*.ts\" && ng lint",
    "build-check": "npm run rm-dist && npm run build && npm run rm-dist",
    "build": "ng build --configuration production",
    "postbuild": "npm run replace-sw-worker",
    "rm-dist": "rm -r -f ./dist",
    "e2e": "ng e2e",
    "serve": "ng serve",
    "serve-dist": "http-server-spa dist/ index.html 4201",
    "bundle-analyser": "npm run rm-dist && ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "generate-token": "firebase login:ci",
    "deploy-frontend": "firebase deploy --debug --project ratatosk-webpage-dev",
    "replace-sw-worker": "rm dist/ngsw-worker.js; cp src/ngsw-worker-custom-angular-v12.js dist/ngsw-worker.js;",
    "set-project-to-internal": "npm run func-set-project internal",
    "set-project-to-public": "npm run func-set-project public",
    "func-set-project": "func() { cd ..; ./setFirebaseProject.sh \"$1\"; cd frontend/src/environments/service-workers; ./setSwWorker.sh \"$1\";}; func"
  },
  "private": true,
  "dependencies": {
    "@angular-material-extensions/password-strength": "8.2.1",
    "@angular/animations": "^12.2.2",
    "@angular/cdk": "^12.2.2",
    "@angular/cli": "^12.2.2",
    "@angular/common": "^12.2.2",
    "@angular/compiler": "^12.2.2",
    "@angular/core": "^12.2.2",
    "@angular/fire": "^6.1.5",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "^12.2.2",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "^12.2.2",
    "@angular/platform-browser-dynamic": "^12.2.2",
    "@angular/pwa": "0.1001.4",
    "@angular/router": "^12.2.2",
    "@angular/service-worker": "^12.2.2",
    "@fortawesome/angular-fontawesome": "0.7.0",
    "@fortawesome/fontawesome-svg-core": "1.2.31",
    "@fortawesome/free-brands-svg-icons": "5.15.0",
    "@fortawesome/free-solid-svg-icons": "5.15.0",
    "@types/mathjs": "6.0.9",
    "angular": "1.8.2",
    "core-js": "3.6.5",
    "d3": "6.2.0",
    "firebase": "8.10.0",
    "hammerjs": "^2.0.8",
    "material-icons": "^0.6.4",
    "mathjs": "8.1.0",
    "ngx-auth-firebaseui": "5.3.0",
    "rxjs": "6.6.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.2",
    "@angular/compiler-cli": "^12.2.2",
    "@angular/language-service": "^12.2.2",
    "@types/d3": "~5.16.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "14.17.5",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "codelyzer": "^6.0.0",
    "concat": "1.0.3",
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-unused-imports": "0.1.2",
    "http-server-spa": "1.3.0",
    "jasmine-core": "3.6.0",
    "jasmine-marbles": "0.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
